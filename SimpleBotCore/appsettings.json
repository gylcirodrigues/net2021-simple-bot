{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MongoDBConnection": {
      "AuthSource": "",
      "Database": "Chatbot",
      "Enabled": true,
      "Host": "localhost",
      "Port": 27017,
      "ReplicaSet": "",
      "RetryWrites": true,
      "UseSSL": true,
